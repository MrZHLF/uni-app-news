<view class="detail"><view class="detail-title">{{formData.title}}</view><view class="detail-header"><view class="detail-header_logo"><image src="{{formData.author.avatar}}" mode="aspectFill"></image></view><view class="detail-header_content"><view class="detail-header_content-title">{{formData.author.author_name}}</view><view class="detail-header_content-info"><text>{{formData.create_time}}</text><text>{{formData.browse_count+" 浏览"}}</text><text>{{formData.thumbs_up_count+" 赞"}}</text></view></view><button class="detail-header_button" type="default" data-event-opts="{{[['tap',[['follow',['$0'],['formData.author.id']]]]]}}" onTap="__e">{{formData.is_author_like?'取消关注':'关注'}}</button></view><view class="detail-content"><view class="detail-html"><u-parse vue-id="78cf7796-1" content="{{formData.content}}" noData="{{noData}}" onVueInit="__l"></u-parse></view><view class="detail-comment"><view class="comment-title">最新评论</view><block a:for="{{commentsList}}" a:for-item="item" a:for-index="__i0__" a:key="comment_id"><view class="comment-content"><comments-box vue-id="{{'78cf7796-2-'+__i0__}}" comments="{{item}}" data-event-opts="{{[['^reply',[['reply']]]]}}" onReply="__e" onVueInit="__l"></comments-box></view></block></view></view><view class="detail-bottom"><view data-event-opts="{{[['tap',[['openComment',['$event']]]]]}}" class="detail-bottom_icons-input" onTap="__e"><text>谈谈你的看法</text><uni-icons vue-id="78cf7796-3" type="compose" size="16" color="#f07373" onVueInit="__l"></uni-icons></view><view class="detail-bottom_icons"><view data-event-opts="{{[['tap',[['open',['$event']]]]]}}" class="detail-bottom_icons-box" onTap="__e"><uni-icons vue-id="78cf7796-4" type="chat" size="22" color="#f07373" onVueInit="__l"></uni-icons></view><view data-event-opts="{{[['tap',[['likeTap',['$0'],['formData._id']]]]]}}" class="detail-bottom_icons-box" onTap="__e"><uni-icons vue-id="78cf7796-5" type="{{formData.is_like?'heart-filled':'heart'}}" size="22" color="#f07373" onVueInit="__l"></uni-icons></view><view data-event-opts="{{[['tap',[['thumbsup',['$0'],['formData._id']]]]]}}" class="detail-bottom_icons-box" onTap="__e"><uni-icons vue-id="78cf7796-6" type="{{formData.is_thumbs_up?'hand-thumbsup-filled':'hand-thumbsup'}}" size="22" color="#f07373" onVueInit="__l"></uni-icons></view></view></view><uni-popup vue-id="78cf7796-7" type="bottom" maskClick="{{false}}" ref="__r" data-ref="popup" onVueInit="__l"><view class="popup-wrap"><view class="popup-header"><text data-event-opts="{{[['tap',[['close',['$event']]]]]}}" class="popup-header_item" onTap="__e">取消</text><text data-event-opts="{{[['tap',[['submit',['$event']]]]]}}" class="popup-header_item" onTap="__e">发布</text></view><view class="popup-content"><textarea class="popup-textarea" placeholder="请输入评论内容" fixed="{{true}}" maxlength="200" data-event-opts="{{[['input',[['__set_model',['','commentsValue','$event',[]]]]]]}}" value="{{commentsValue}}" onInput="__e"></textarea><view class="popup-count">{{commentsValue.length+"/200"}}</view></view></view></uni-popup></view>