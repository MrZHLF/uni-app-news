<view class="home"><navbar vue-id="8c37d7b0-1" isSearch="{{true}}" value="{{value}}" data-event-opts="{{[['^input',[['__set_model',['','value','$event',[]]],['change']]]]}}" onInput="__e" onVueInit="__l"></navbar><block a:if="{{is_history}}"><view class="home-list"><view class="label-box"><view class="label-header"><text class="label-title">搜索历史</text><text data-event-opts="{{[['tap',[['clear',['$event']]]]]}}" class="label-clear" onTap="__e">清空</text></view><block a:if="{{historyList.length>0}}"><view class="label-content"><block a:for="{{historyList}}" a:for-item="item" a:for-index="index" a:key="index"><view data-event-opts="{{[['tap',[['openHistory',['$0'],[[['historyList','',index]]]]]]]}}" class="label-content-item" onTap="__e">{{item.name}}</view></block></view></block><block a:else><view class="no-data">没有搜索历史</view></block></view></view></block><block a:else><list-scroll class="list-scroll" vue-id="8c37d7b0-2" onVueInit="__l"><block a:if="{{loading}}"><uni-load-more vue-id="{{('8c37d7b0-3')+','+('8c37d7b0-2')}}" status="loading" iconType="snow" onVueInit="__l"></uni-load-more></block><block a:if="{{searchList.length>0}}"><view><block a:for="{{searchList}}" a:for-item="item" a:for-index="index" a:key="_id"><list-card vue-id="{{('8c37d7b0-4-'+index)+','+('8c37d7b0-2')}}" item="{{item}}" data-event-opts="{{[['^click',[['setHistory']]]]}}" onClick="__e" onVueInit="__l"></list-card></block></view></block><block a:if="{{searchList.length==0&&!loading}}"><view class="no-data">没有搜索到相关数据</view></block></list-scroll></block></view>