<view class="label"><view class="label-box"><view class="label-header"><view class="label-title">我的标签</view><view data-event-opts="{{[['tap',[['editLabel',['$event']]]]]}}" class="label-edit" bindtap="__e">{{is_edit?'完成':'编辑'}}</view></view><block tt:if="{{loading}}"><uni-load-more vue-id="6188261f-1" status="loading" iconType="snow" bind:__l="__l"></uni-load-more></block><block tt:if="{{!loading}}"><view class="label-content"><block tt:for="{{labelList}}" tt:for-item="item" tt:for-index="index" tt:key="_id"><view class="label-content_item">{{''+item.name+''}}<block tt:if="{{is_edit}}"><uni-icons class="icon-close" vue-id="{{'6188261f-2-'+index}}" type="clear" size="20" color="red" data-event-opts="{{[['^click',[['del',[index]]]]]}}" bind:click="__e" bind:__l="__l"></uni-icons></block></view></block><block tt:if="{{labelList.length===0&&!loading}}"><view class="no-data">当前没有数据</view></block></view></block></view><view class="label-box"><view class="label-header"><view class="label-title">推荐标签</view></view><block tt:if="{{loading}}"><uni-load-more vue-id="6188261f-3" status="loading" iconType="snow" bind:__l="__l"></uni-load-more></block><block tt:if="{{!loading}}"><view class="label-content"><block tt:for="{{list}}" tt:for-item="item" tt:for-index="index" tt:key="_id"><view data-event-opts="{{[['tap',[['add',[index]]]]]}}" class="label-content_item" bindtap="__e">{{item.name}}</view></block><block tt:if="{{list.length===0&&!loading}}"><view class="no-data">当前没有数据</view></block></view></block></view></view>