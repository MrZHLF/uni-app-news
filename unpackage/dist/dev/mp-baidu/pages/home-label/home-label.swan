<view class="label"><view class="label-box"><view class="label-header"><view class="label-title">我的标签</view><view data-event-opts="{{[['tap',[['editLabel',['$event']]]]]}}" class="label-edit" bindtap="__e">{{is_edit?'完成':'编辑'}}</view></view><block s-if="{{loading}}"><uni-load-more vue-id="6187bb91-1" status="loading" iconType="snow"></uni-load-more></block><block s-if="{{!loading}}"><view class="label-content"><block s-for="{{labelList}}" s-for-item="item" s-for-index="index" s-key="_id"><view class="label-content_item">{{''+item.name+''}}<block s-if="{{is_edit}}"><uni-icons class="icon-close" vue-id="{{'6187bb91-2-'+index}}" type="clear" size="20" color="red" data-event-opts="{{[['^click',[['del',[index]]]]]}}" bind:click="__e"></uni-icons></block></view></block><block s-if="{{labelList.length===0&&!loading}}"><view class="no-data">当前没有数据</view></block></view></block></view><view class="label-box"><view class="label-header"><view class="label-title">推荐标签</view></view><block s-if="{{loading}}"><uni-load-more vue-id="6187bb91-3" status="loading" iconType="snow"></uni-load-more></block><block s-if="{{!loading}}"><view class="label-content"><block s-for="{{list}}" s-for-item="item" s-for-index="index" s-key="_id"><view data-event-opts="{{[['tap',[['add',[index]]]]]}}" class="label-content_item" bindtap="__e">{{item.name}}</view></block><block s-if="{{list.length===0&&!loading}}"><view class="no-data">当前没有数据</view></block></view></block></view></view>